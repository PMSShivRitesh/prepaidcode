 var d = new Date();

var currDate = d.getDate();
var currMonth = d.getMonth();
var currYear = d.getFullYear();

if(currDate<10){
	currDate="0"+currDate;
}
if((currMonth+1)<10){
	currMonth="0"+(currMonth+1);
}

var dateStr = currDate + "/" + currMonth + "/" + currYear;


$('#VisitDate').datepicker({
    autoclose: true,
    todayHighlight: true,
    format: "dd/mm/yyyy",
    defaultDate: dateStr
}); 


$('#VisitDate').val(dateStr);





